        // "enum"          : "#FF0000",
        // "enumMember"    : "#FF0000",
        // "event"         : "#FF0000",
        // "function"      : "#FF0000",
        // "interface"     : "#FF0000",
        // "keyword"       : "#FF0000",
        // "label"         : "#FF0000",
        // "macro"         : "#FF0000",
        // "member"        : "#FF0000",
        // "namespace"     : "#FF0000",
        // "number"        : "#FF0000",
        // "operator"      : "#FF0000",
        // "parameter"     : "#FF0000",
        // "property"      : "#FF0000",
        // "regexp"        : "#FF0000",
        // "string"        : "#FF0000",
        // "struct"        : "#FF0000",
        // "type"          : "#FF0000",
        // "typeParameter" : "#FF0000",
        // "variable"      : "#FF0000",

        "rules": {
            // "enum"       : "#FF0000"
            // "enumMember" : "#FF0000"
            // "method"     : "#FF0000"
        }
    },

    // Tokens Customization
    "editor.tokenColorCustomizations"            : {
        // "semanticHighlighting": true, // Not allowed anymore
        // "comments"            : "#5C6773",
        // "functions"           : "#FF0000",
        // "keywords"            : "#FF0000",
        // "numbers"             : "#FF0000",
        // "strings"             : "#FFF176",
        // "types"               : "#FF0000",
        "variables"           : "#ffffff",

        // Text Mate Rules
        "textMateRules": [
            {
                // Property name ("files.something")
                "scope":
                [
                    "support.type.property-name",
                    "entity.name.tag",
                ],
                "settings":
                {
                    "foreground": "#BDBDBD",
                },
            },
            {
                // Constants (true, false etc.)
                "scope":
                [
                    "constant.numeric",
                    "constant.language",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle" : "bold",
                }
            },
            {
                // Comment
                "scope":
                [
                    "comment",
                    "punctuation.definition.comment",
                ],
                "settings":
                {
                    "foreground": "#5d6874bd",
                    "fontStyle" : "",
                }
            },
            {
                // String
                "scope":
                [
                    "string",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                }
            },
            {
                // Keyword
                "scope":
                [
                    "keyword"
                ],
                "settings":
                {
                    "foreground": "#FFE0B2"
                }
            },
            {
                // Function (printf, source, alias etc.)
                "scope":
                [
                    "entity.name.function",
                ],
                "settings":
                {
                    "foreground": "#50FA7B"
                }
            },
            {
                // Support Type
                "scope":
                [
                    "support.type",
                ],
                "settings":
                {
                    "foreground": "#50FA7B"
                }
            },
            {
                // Support Class
                "scope":
                [
                    "support.class"
                ],
                "settings":
                {
                    "foreground": "#ffffff"
                }
            },
            {
                // Storage Type
                "scope":
                [
                    "storage.type"
                ],
                "settings":
                {
                    "foreground": "#CCCAC2",
                    "fontStyle": "italic"
                }
            },
            //========================================================================================
            // HTML
            //========================================================================================
            {
                // </name>
                "scope":
                [
                    "punctuation.definition.tag.begin.html",
                    "punctuation.definition.tag.end.html",
                    "entity.name.tag.html"
                ],
                "settings":
                {
                    "foreground": "#ffffff40",
                    "fontStyle": "bold"
                }
            },
            {
                // Attribute like: class=
                "scope":
                [
                    "entity.other.attribute-name.html",
                ],
                "settings":
                {
                    "foreground": "#50FA7B",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " "
                "scope":
                [
                    "punctuation.definition.string.begin.html",
                    "punctuation.definition.string.end.html",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Operator and sign equal [=]
                "scope":
                [
                    "punctuation.separator.key-value.html",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // SMARTY
            //========================================================================================
            {
                // if, foreach, block etc.
                "scope":
                [
                    "keyword.control.smarty",
                    "support.function.built-in.smarty",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Parameter like: params=
                "scope":
                [
                    "variable.parameter.smarty",
                ],
                "settings":
                {
                    "foreground": "#8D6E63",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " " and brackets { }
                "scope":
                [
                    "punctuation.definition.string.begin.smarty",
                    "punctuation.definition.string.end.smarty",
                    "punctuation.section.embedded.begin.smarty",
                    "punctuation.section.embedded.end.smarty",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Smarty variable properties $variable.[properties]
                "scope":
                [
                    "punctuation.definition.variable.smarty",
                    "variable.other.smarty",
                    "source.smarty"
                ],
                "settings":
                {
                    "foreground": "#E0E0E0",
                    "fontStyle": "bold"
                }
            },
            {
                // String betwen quote " example text"
                "scope":
                [
                    "string.quoted.single.smarty",
                    "string.quoted.double.smarty",
                ],
                "settings":
                {
                    //"foreground": "#CBCCC6",
                    "fontStyle": ""
                }
            },
            {
                // Operator and sign equal [=]
                "scope":
                [
                    "keyword.operator.smarty",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
