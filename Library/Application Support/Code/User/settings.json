            //
            //  Text:
            //  #CFD8DC - Text Foreground
            //
            // ----------------------------------------------------------------
            // ActivityBar
            "activityBar.background"                              : "#181E2B",
            "activityBar.border"                                  : "#181E2B",
            "activityBar.dropBackground"                          : "#ff0000",
