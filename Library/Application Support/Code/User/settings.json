{
    // System Settings

    //---------------------------------------
    // Breadcrumbs
    //---------------------------------------
    "breadcrumbs.enabled"           : true,

    //---------------------------------------
    // Editor
    //---------------------------------------

    // Cursor
    "editor.cursorBlinking"         : "smooth",
    "editor.cursorStyle"            : "line",

    // Font
    "editor.fontFamily"             : "FiraCode-Retina",   // v6.2
    "editor.fontLigatures"          : false,
    "editor.fontSize"               : 13,                  //  13 - Default
    "editor.fontWeight"             : "400",               // 400 - Default || 300 - Light, 400 - Regular, 500 - Medium, 600 - Bold
    "editor.letterSpacing"          : 0,
    "editor.lineHeight"             : 14,                  //  19 - Default

    // General
    "editor.lineNumbers"            : "on",
    "editor.minimap.enabled"        : false,
    "editor.minimap.scale"          : 2,
    "editor.overviewRulerBorder"    : false,

    // Render
    "editor.renderControlCharacters": true,
    // "editor.renderIndentGuides"     : false,
    "editor.renderLineHighlight"    : "all",
    "editor.renderWhitespace"       : "all",
    "editor.rulers"                 : [],

    // Scroll
    "editor.scrollBeyondLastColumn" : 5,
    "editor.scrollBeyondLastLine"   : true,
    "editor.smoothScrolling"        : true,

    // Snippets
    "editor.snippetSuggestions"     : "top",

    // Indentation
    "editor.tabSize"                : 2,

    // Word wrapping
    "editor.wordWrap"               : "off",
    "editor.wordWrapColumn"         : 160,

    // Brackets
    "editor.matchBrackets"          : "never",
    "editor.autoClosingBrackets"    : "always",

    // Tokens Experimental
    "editor.semanticTokenColorCustomizations": {
        // "class"         : "#ff0000",
        // "comment"       : "#ff0000",
        // "enum"          : "#ff0000",
        // "enumMember"    : "#ff0000",
        // "event"         : "#ff0000",
        // "function"      : "#ff0000",
        // "interface"     : "#ff0000",
        // "keyword"       : "#ff0000",
        // "label"         : "#ff0000",
        // "macro"         : "#ff0000",
        // "member"        : "#ff0000",
        // "namespace"     : "#ff0000",
        // "number"        : "#ff0000",
        // "operator"      : "#ff0000",
        // "parameter"     : "#ff0000",
        // "property"      : "#ff0000",
        // "regexp"        : "#ff0000",
        // "string"        : "#ff0000",
        // "struct"        : "#ff0000",
        // "type"          : "#ff0000",
        // "typeParameter" : "#ff0000",
        // "variable"      : "#ff0000",

        "rules": {
            // "enum": "#ff0000"
            // "method": "#ff0000"
            // "enumMember": "#ff0000"
        }
    },

    // Tokens Customization
    "editor.tokenColorCustomizations"            : {
        // "semanticHighlighting": true, // Not allowed anymore
        // "comments"            : "#5C6773",
        // "functions"           : "#ff0000",
        // "keywords"            : "#ff0000",
        // "numbers"             : "#ff0000",
        // "strings"             : "#FFF176",
        // "types"               : "#ff0000",
        "variables"           : "#ffffff",

        // Text Mate Rules
        "textMateRules": [
            {
                // Property name ("files.something")
                "scope":
                [
                    "support.type.property-name",
                    "entity.name.tag",
                ],
                "settings":
                {
                    "foreground": "#BDBDBD",
                },
            },
            {
                // Constants (true, false etc.)
                "scope":
                [
                    "constant.numeric",
                    "constant.language",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle" : "bold",
                }
            },
            {
                // Comment
                "scope":
                [
                    "comment",
                    "punctuation.definition.comment",
                ],
                "settings":
                {
                    "foreground": "#5d6874bd",
                    "fontStyle" : "",
                }
            },
            {
                // String
                "scope":
                [
                    "string",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                }
            },
            {
                // Keyword
                "scope":
                [
                    "keyword"
                ],
                "settings":
                {
                    "foreground": "#FFE0B2"
                }
            },
            {
                // Function (printf, source, alias etc.)
                "scope":
                [
                    "entity.name.function",
                ],
                "settings":
                {
                    "foreground": "#50FA7B"
                }
            },
            {
                // Support Type
                "scope":
                [
                    "support.type",
                ],
                "settings":
                {
                    "foreground": "#50FA7B"
                }
            },
            {
                // Support Class
                "scope":
                [
                    "support.class"
                ],
                "settings":
                {
                    "foreground": "#ffffff"
                }
            },
            {
                // Storage Type
                "scope":
                [
                    "storage.type"
                ],
                "settings":
                {
                    "foreground": "#CCCAC2",
                    "fontStyle": "italic"
                }
            },
            //========================================================================================
            // HTML
            //========================================================================================
            {
                // </name>
                "scope":
                [
                    "punctuation.definition.tag.begin.html",
                    "punctuation.definition.tag.end.html",
                    "entity.name.tag.html"
                ],
                "settings":
                {
                    "foreground": "#ffffff40",
                    "fontStyle": "bold"
                }
            },
            {
                // Attribute like: class=
                "scope":
                [
                    "entity.other.attribute-name.html",
                ],
                "settings":
                {
                    "foreground": "#50FA7B",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " "
                "scope":
                [
                    "punctuation.definition.string.begin.html",
                    "punctuation.definition.string.end.html",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Operator and sign equal [=]
                "scope":
                [
                    "punctuation.separator.key-value.html",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // SMARTY
            //========================================================================================
            {
                // if, foreach, block etc.
                "scope":
                [
                    "keyword.control.smarty",
                    "support.function.built-in.smarty",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Parameter like: params=
                "scope":
                [
                    "variable.parameter.smarty",
                ],
                "settings":
                {
                    "foreground": "#8D6E63",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " " and brackets { }
                "scope":
                [
                    "punctuation.definition.string.begin.smarty",
                    "punctuation.definition.string.end.smarty",
                    "punctuation.section.embedded.begin.smarty",
                    "punctuation.section.embedded.end.smarty",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Smarty variable properties $variable.[properties]
                "scope":
                [
                    "punctuation.definition.variable.smarty",
                    "variable.other.smarty",
                    "source.smarty"
                ],
                "settings":
                {
                    "foreground": "#E0E0E0",
                    "fontStyle": "bold"
                }
            },
            {
                // String betwen quote " example text"
                "scope":
                [
                    "string.quoted.single.smarty",
                    "string.quoted.double.smarty",
                ],
                "settings":
                {
                    //"foreground": "#CBCCC6",
                    "fontStyle": ""
                }
            },
            {
                // Operator and sign equal [=]
                "scope":
                [
                    "keyword.operator.smarty",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // JS - JavaScript
            //========================================================================================
            {
                "scope":
                [
                    "keyword.control.import.js",
                    "keyword.control.from.js",
                ],
                "settings":
                {
                    "foreground": "#E0E0E0",
                    "fontStyle": ""
                }
            },
            {
                "scope":
                [
                    "entity.name.type.js",
                    "entity.name.function.js",
                    "entity.name.function.tagged-template.js",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                "scope":
                [
                    "constant.character.escape.backslash.regexp",
                    "keyword.control.conditional.js",
                    "keyword.control.default.js",
                    "keyword.control.export.js",
                    "keyword.control.flow.js",
                    "keyword.control.loop.js",
                    "keyword.control.module.js",
                    "keyword.control.module.reference.js",
                    "keyword.operator.arithmetic.js",
                    "keyword.operator.assignment.compound.js",
                    "keyword.operator.assignment.js",
                    "keyword.operator.comparison.js",
                    "keyword.operator.expression.in.js",
                    "keyword.operator.expression.typeof.js",
                    "keyword.operator.increment.js",
                    "keyword.operator.logical.js",
                    "keyword.operator.new.js",
                    "keyword.operator.quantifier.regexp",
                    "keyword.operator.relational.js",
                    "keyword.operator.ternary.js",
                    "meta.brace.round.js",
                    "meta.brace.square.js",
                    "meta.delimiter.comma.js",
                    "punctuation.accessor.js",
                    "punctuation.definition.block.js",
                    "punctuation.definition.parameters.begin.js",
                    "punctuation.definition.parameters.end.js",
                    "punctuation.separator.comma.js",
                    "storage.type.class.js",
