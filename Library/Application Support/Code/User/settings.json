    //---------------------------------------
    // File associations
    //---------------------------------------
    "files.associations"          : {
        ".env.jenkins"            : "dotenv",
    },
    "files.defaultLanguage"       : "plaintext",
    "files.trimFinalNewlines"     : true,
    "files.trimTrailingWhitespace": true,

    //---------------------------------------
    // Git
    //---------------------------------------
    "git.autofetch"               : true,
    "git.enableSmartCommit"       : true,

    //---------------------------------------
    // Search
    //---------------------------------------
    "search.exclude": {
        "**/bower_components" : true,
        "**/node_modules"     : true,
        "*.min.js"            : true,
        ".build/**"           : true,
        "extensions/**/out/**": true,
        "i18n/**"             : true,
        "out*/**"             : true,
    },

    //---------------------------------------
    // Terminal
    //---------------------------------------
    "terminal.external.osxExec"              : "Alacritty.app",
