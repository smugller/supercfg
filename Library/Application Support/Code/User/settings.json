    //+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
{
    // System Settings

    //---------------------------------------
    // Breadcrumbs
    //---------------------------------------
    "breadcrumbs.enabled"           : false,

    //---------------------------------------
    // Editor
    //---------------------------------------

    // Cursor
    "editor.cursorBlinking"         : "smooth",
    "editor.cursorStyle"            : "line",

    // Font
    "editor.fontFamily"             : "FiraCode-Retina",   // v3.1
    "editor.fontLigatures"          : true,
    "editor.fontSize"               : 13,
    "editor.fontWeight"             : "400",               // 300 - Light, 400 - Regular, 500 - Medium, 600 - Bold
    "editor.letterSpacing"          : 0,
    "editor.lineHeight"             : 19,

    // General
    "editor.lineNumbers"            : "on",
    "editor.minimap.enabled"        : true,
    "editor.minimap.scale"          : 2,
    "editor.overviewRulerBorder"    : false,

    // Render
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides"     : false,
    "editor.renderLineHighlight"    : "all",
    "editor.renderWhitespace"       : "selection",
    "editor.rulers"                 : [],

    // Scroll
    "editor.scrollBeyondLastColumn" : 5,
    "editor.scrollBeyondLastLine"   : true,
    "editor.smoothScrolling"        : true,

    // Snippets
    "editor.snippetSuggestions"     : "top",

    // Indentation
    "editor.tabSize"                : 2,

    // Word wrapping
    "editor.wordWrap"               : "off",
    "editor.wordWrapColumn"         : 160,

    // Brackets
    "editor.matchBrackets"          : "never",
    "editor.autoClosingBrackets"    : "always",

    // Tokens Experimental
    "editor.semanticTokenColorCustomizations": {
        // "class"         : "#ff0000",
        // "comment"       : "#ff0000",
        // "enum"          : "#ff0000",
        // "enumMember"    : "#ff0000",
        // "event"         : "#ff0000",
        // "function"      : "#ff0000",
        // "interface"     : "#ff0000",
        // "keyword"       : "#ff0000",
        // "label"         : "#ff0000",
        // "macro"         : "#ff0000",
        // "member"        : "#ff0000",
        // "namespace"     : "#ff0000",
        // "number"        : "#ff0000",
        // "operator"      : "#ff0000",
        // "parameter"     : "#ff0000",
        // "property"      : "#ff0000",
        // "regexp"        : "#ff0000",
        // "string"        : "#ff0000",
        // "struct"        : "#ff0000",
        // "type"          : "#ff0000",
        // "typeParameter" : "#ff0000",
        // "variable"      : "#ff0000",
    },

    // Tokens Customization
    "editor.tokenColorCustomizations"            : {
        // "semanticHighlighting": true, // Not allowed anymore
        // "comments"            : "#5C6773",
        // "functions"           : "#ff0000",
        // "keywords"            : "#ff0000",
        // "numbers"             : "#ff0000",
        // "strings"             : "#FFF176",
        // "types"               : "#ff0000",
        // "variables"           : "#ff0000",

        // Text Mate Rules
        "textMateRules": [
            // Property name ("files.something")
            {
                "scope":
                [
                    "support.type.property-name",
                    "entity.name.tag",
                ],
                "settings":
                {
                    "foreground": "#BDBDBD",
                },
            },
            // Constants (true, false etc.)
            {
                "scope":
                [
                    "constant.numeric",
                    "constant.language",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle" : "bold",
                }
            },
            // Comment
            {
                "scope":
                [
                    "comment",
                    "punctuation.definition.comment",
                ],
                "settings":
                {
                    "foreground": "#5d6874bd",
                }
            },
            // String
            {
                "scope":
                [
                    "string",
                ],
                "settings":
                {
                    //"foreground": "#FFF59D",
                    "foreground": "#FFF59D"
                }
            },
            // Keyword
            {
                "scope":
                [
                    "keyword"
                ],
                "settings":
                {
                    "foreground": "#FFE0B2"
                }
            },
            // Function (printf, source, alias etc.)
            {
                "scope":
                [
                    "entity.name.function",
                ],
                "settings":
                {
                    //"foreground": "#50FA7B"
                }
            },
            // Support Class
            {
                "scope":
                [
                    "support.class"
                ],
                "settings":
                {
                    "foreground": "#ffffff"
                }
            },
            //========================================================================================
            // HTML
            //========================================================================================
            {
                // </name>
                "scope":
                [
                    "punctuation.definition.tag.begin",
                    "punctuation.definition.tag.end",
                    "entity.name.tag.html"
                ],
                "settings":
                {
                    "foreground": "#ffffff40",
                    "fontStyle": "bold"
                }
            },
            {
                // Attribute like: class=
                "scope":
                [
                    "entity.other.attribute-name",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " "
                "scope":
                [
                    "punctuation.definition.string.begin.html",
                    "punctuation.definition.string.end.html",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // SMARTY
            //========================================================================================
            {
                // if, foreach etc.
                "scope":
                [
                    "keyword.control.smarty",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Parameter like: params=
                "scope":
                [
                    "variable.parameter.smarty",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " " and brackets { }
                "scope":
                [
                    "punctuation.definition.string.begin.smarty",
                    "punctuation.definition.string.end.smarty",
                    "punctuation.section.embedded.begin.smarty",
                    "punctuation.section.embedded.end.smarty",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Smarty variable properties $variable.[properties]
                "scope":
                [
                    "punctuation.definition.variable.smarty",
                    "variable.other.smarty",
                    "source.smarty"
                ],
                "settings":
                {
                    "foreground": "#ffffff99",
                    "fontStyle": "bold"
                }
            },
            {
                // String betwen quote " example text"
                "scope":
                [
                    "string.quoted.single.smarty",
                    "string.quoted.double.smarty",
                ],
                "settings":
                {
                    //"foreground": "#CBCCC6",
                    "fontStyle": ""
                }
            },
            {
                // Operator and sign equal [=]
                "scope":
                [
                    "keyword.operator.smarty",
                    "punctuation.separator.key-value.html"
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // JS - JavaScript
            //========================================================================================
            {
                // Keywords like [Import], [From] etc.
                "scope":
                [
                    "keyword.control.import.js",
                    "keyword.control.from.js",
                ],
                "settings":
                {
                    "foreground": "#E0E0E0",
                    "fontStyle": ""
                }
            },
            {
                // Import [variable] from ...
                "scope":
                [
                    "variable.other.readwrite.alias.js",
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle": ""
                }
            },
            {
                // [.function] [.find] etc.
                "scope":
                [
                    "entity.name.type.js",
                    "entity.name.function.js",
                ],
                "settings":
                {
                    //"foreground": "#F0F4C3",
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Keywords [const] [let] [new] [in] etc.
                "scope":
                [
                    "keyword.control.conditional.js",
                    "keyword.control.flow.js",
                    "keyword.control.loop.js",
                    "keyword.operator.arithmetic.js",
                    "keyword.operator.assignment.js",
                    "keyword.operator.comparison.js",
                    "keyword.operator.expression.in.js",
                    "keyword.operator.expression.typeof.js",
                    "keyword.operator.increment.js",
                    "keyword.operator.logical.js",
                    "keyword.operator.new.js",
                    "storage.type.function.arrow.js",
                    "storage.type.function.js",
                    "storage.type.js",
                    "variable.language.this.js",
                    "support.function.js",
                    "support.type.object.module.js",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Decimals and numbers
                "scope":
                [
                    "constant.numeric.decimal.js",
                    "constant.language.null.js",
                    "constant.language.undefined.js"
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle": ""
                }
            },
            {
                // Single, double and template quote punctuation ' ', " ", ` `
                "scope":
                [
                    "punctuation.definition.string.begin.js",
                    "punctuation.definition.string.end.js",
                    "punctuation.definition.string.template.begin.js",
                    "punctuation.definition.string.template.end.js",
                    "punctuation.terminator.statement.js"
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Dot .
                "scope":
                [
                    "punctuation.accessor.js",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Brackets () [] {}
                "scope":
                [
                    "meta.brace.round.js",
                    "meta.brace.square.js",
                    "punctuation.definition.parameters.begin.js",
                    "punctuation.definition.parameters.end.js",
                    "punctuation.definition.block.js"
                ],
                "settings":
                {
                    //"foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Comma ,
                "scope":
                [
                    "punctuation.separator.comma.js",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Comma ;
                "scope":
                [
                    "punctuation.terminator.statement.js"
                ],
                "settings":
                {
                    "foreground": "#9E9E9E",
                    "fontStyle": "bold"
                }
            },
            {
                // Object
                "scope":
                [
                    "support.variable.property.js",
                    "variable.other.constant.js",
                    "variable.other.object.js",
                    "variable.other.object.property.js",
                    "variable.other.property.js",
                    "variable.other.readwrite.js",
                    "variable.parameter.js",
                ],
                "settings":
                {
                    "foreground": "#ffffffda",
                    "fontStyle": ""
                }
            },
            {
                // [Key][:]: Value
                "scope":
                [
                    "meta.object-literal.key.js",
                    "punctuation.separator.key-value.js",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            //========================================================================================
            // SCSS / SASS
            //========================================================================================
            {
                // [@import]
                "scope":
                [
                    "keyword.control.at-rule.import.scss",
                    "support.function.misc.scss",
                    "entity.name.function.scss",
                    "entity.name.tag.css"
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Quote " "
                "scope":
                [
                    "punctuation.definition.string.begin.scss",
                    "punctuation.definition.string.end.scss"
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // ;
                "scope":
                [
                    "punctuation.terminator.rule.css",
                ],
                "settings":
                {
                    "foreground": "#9E9E9E",
                    "fontStyle": "bold"
                }
            },
            {
                // $variable
                "scope":
                [
                    "variable.scss",
                ],
                "settings":
                {
                    "foreground": "#ffffff99",
                    "fontStyle": "bold"
                }
            },
            {
                // Property name
                "scope":
                [
                    "support.type.property-name.css"
                ],
                "settings":
                {
                    "foreground": "#ffffff99",
                    "fontStyle": ""
                }
            },
            {
                // ( ) and : and [ ] and [.]something
                "scope":
                [
                    "punctuation.definition.map.begin.bracket.round.scss",
                    "punctuation.definition.map.end.bracket.round.scss",
                    "meta.definition.variable.map.scss",
                    "punctuation.definition.attribute-selector.begin.bracket.square.scss",
                    "punctuation.definition.attribute-selector.end.bracket.square.scss",
                    "punctuation.definition.entity.css"

                    //"punctuation.section.function.scss"
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // Numeric values of variables and !important
                "scope":
                [
                    "constant.numeric.css",
                    "keyword.other.important.scss"
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle": "bold"
                }
            },
            {
                // Units like px, ms, rem etc.
                "scope":
                [
                    "keyword.other.unit.deg.css",
                    "keyword.other.unit.em.css",
                    "keyword.other.unit.ms.css",
                    "keyword.other.unit.percentage.css",
                    "keyword.other.unit.px.css",
                    "keyword.other.unit.rem.css",
                    "keyword.other.unit.s.css",
                    "keyword.other.unit.vw.css",
                    "support.constant.mathematical-symbols.scss"
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // Constants like: none
                "scope":
                [
                    "support.constant.property-value.css",
                    "support.constant.vendored.property-value.css",
                    "meta.definition.variable.scss"
                ],
                "settings":
                {
                    "foreground": "#CE93D8",
                    "fontStyle": "bold"
                }
            },
            {
                // #{ and }
                "scope":
                [
                    "punctuation.definition.interpolation.begin.bracket.curly.scss",
                    "punctuation.definition.interpolation.end.bracket.curly.scss",
                    "meta.property-list.scss"
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },
            {
                // String betwen quote " example text "
                "scope":
                [
                    "string.quoted.single.scss",
                    "variable.parameter.url.scss"
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                }
            },
            {
                // brackets { }
                "scope":
                [
                    "punctuation.section.property-list.begin.bracket.curly.scss",
                    "punctuation.section.property-list.end.bracket.curly.scss"
                ],
                "settings":
                {
                    "foreground": "#ffffff99",
                    "fontStyle": ""
                }
            },
            {
                // something [>] something
                "scope":
                [
                    "source.css.scss"
                ],
                "settings":
                {
                    "foreground": "#FFE0B2",
                    "fontStyle": "bold"
                }
            },
            {
                // Class name normal
                "scope":
                [
                    "entity.other.attribute-name.class.css",
                ],
                "settings":
                {
                    "foreground": "#FFE0B2",
                    "fontStyle": "bold"
                }
            },
            {
                // PseudoClass name
                "scope":
                [
                    "entity.other.attribute-name.pseudo-class.css"
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // ID name normal
                "scope":
                [
                    "entity.other.attribute-name.id.css",
                ],
                "settings":
                {
                    "foreground": "#FFD54F",
                    "fontStyle": "bold"
                }
            },
            {
                // Double quoted " "
                "scope":
                [
                    "string.quoted.double.scss"
                ],
                "settings":
                {
                    "foreground": "#FFF59D",
                    "fontStyle": ""
                }
            },
            {
                // Colors like hex etc.
                "scope":
                [
                    "constant.other.color.rgb-value.hex.css"
                ],
                "settings":
                {
                    "foreground": "#00E676",
                    "fontStyle": "bold"
                }
            },
            {
                // Keywords like @each, @extend, @include etc.
                "scope":
                [
                    "entity.name.tag.reference.scss",
                    "entity.name.tag.wildcard.scss",
                    "keyword.control.at-rule.extend.scss",
                    "keyword.control.at-rule.include.scss",
                    "keyword.control.at-rule.media.scss",
                    "keyword.control.at-rule.mixin.scss",
                    "keyword.control.each.scss",
                    "keyword.control.else.scss",
                    "keyword.control.if.scss",
                    "keyword.control.while.scss",
                ],
                "settings":
                {
                    "foreground": "#E57373",
                    "fontStyle": "bold italic"
                }
            },
            {
                // Custom entity tag
                "scope":
                [
                    "entity.name.tag.custom.scss",
                ],
                "settings":
                {
                    "foreground": "#E57373",
                    "fontStyle": "bold"
                }
            },
            {
                // before, after etc.
                "scope":
                [
                    "entity.other.attribute-name.pseudo-element.css",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                // media "screen" word
                "scope":
                [
                    "support.constant.media.css",
                ],
                "settings":
                {
                    "foreground": "#ffffff",
                    "fontStyle": "bold"
                }
            },

            // Blank
            {
                "scope":
                [

                ],
                "settings":
                {

                }
            },
        ]
    },

    //---------------------------------------
    // File associations
    //---------------------------------------
    "files.associations"          : {
        ".env.jenkins"            : "dotenv",
    },
    "files.defaultLanguage"       : "plaintext",
    "files.trimFinalNewlines"     : true,
    "files.trimTrailingWhitespace": true,

    //---------------------------------------
    // Git
    //---------------------------------------
    "git.autofetch"               : true,
    "git.enableSmartCommit"       : true,

    //---------------------------------------
    // Search
    //---------------------------------------
    "search.exclude": {
        "**/bower_components" : true,
        "**/node_modules"     : true,
        "*.min.js"            : true,
        ".build/**"           : true,
        "extensions/**/out/**": true,
        "i18n/**"             : true,
        "out*/**"             : true,
    },

    //---------------------------------------
    // Terminal
    //---------------------------------------
    "terminal.external.osxExec"              : "Hyper.app",
    "terminal.integrated.automationShell.osx": "/usr/local/bin/zsh",
    "terminal.integrated.fontFamily"         : "FiraCode Nerd Font",
    "terminal.integrated.fontSize"           : 12,
    "terminal.integrated.lineHeight"         : 1,
    "terminal.integrated.shell.osx"          : "/usr/local/bin/zsh",

    //---------------------------------------
    // Window
    //---------------------------------------
    "window.newWindowDimensions": "default",
    "window.title"              : "${dirty} $PATH: ${activeFolderLong}$/ ${separator} ${activeEditorShort} ${dirty}",
    "window.titleBarStyle"      : "custom",
    "window.zoomLevel"          : -1,

    //---------------------------------------
    // Workbench
    //---------------------------------------
    "workbench.activityBar.visible": true,
    "workbench.colorTheme"         : "Ayu Mirage Bordered",
    "workbench.editor.tabSizing"   : "shrink",
    "workbench.fontAliasing"       : "antialiased",
    "workbench.iconTheme"          : "vscode-great-icons",
    "workbench.sideBar.location"   : "left",
    "workbench.statusBar.visible"  : true,
    "workbench.colorCustomizations": {

            // Color Palette --------------------------------------------------
            //
            //  Main colors:
            //  #FDD835 - Yellow  Background
            //  #FFF176 - Yellow  Hover
            //          - Yellow  Active
            //          - Yellow  Inactive
            //
            //  #181E2B - Dark    Background
            //          - Dark    Hover
            //          - Dark    Active
            //          - Dark    Inactive
            //
            //  #222835 - Editor  Background
            //  #222835 - SideBar Background
            //
            //  Info:
            //  #FF5252 - Error   Color
            //  #B2FF59 - Info    Color
            //  #FFFF00 - Warning Color
            //  #00C853 - Success Color
            //
            //  Text:
            //  #CFD8DC - Text Foreground
            //
            //  Dracula colors: #50FA7B - ultra green
            //
            // Useful: https://github.com/dracula/visual-studio-code/blob/bb709dcaa52914ff0a8b677a9542e198b86f1923/src/dracula.yml
            //
            // ----------------------------------------------------------------
            // ActivityBar
            "activityBar.background"                              : "#181E2B",
            "activityBar.border"                                  : "#181E2B",
            "activityBar.dropBackground"                          : "#ff0000",
            "activityBar.foreground"                              : "#CFD8DC",

            // ActivityBar Inactive
            "activityBar.inactiveForeground"                      : "#CFD8DC",

            // ActivityBaar Active
            "activityBar.activeBackground"                        : "#181E2B",
            "activityBar.activeBorder"                            : "#FDD835",
            "activityBar.activeFocusBorder"                       : "#181E2B",

            // ActivityBar Badge
            "activityBarBadge.background"                         : "#CFD8DC",
            "activityBarBadge.foreground"                         : "#181E2B",

            // Badge
            "badge.background"                                    : "#FDD835",
            "badge.foreground"                                    : "#181E2B",

            // Breadcrumb
            "breadcrumb.activeSelectionForeground"                : "#FDD835",
            "breadcrumb.background"                               : "#181E2B",
            "breadcrumb.focusForeground"                          : "#FDD835",
            "breadcrumb.foreground"                               : "#CFD8DC",

            // Breadcrumb Picker
            "breadcrumbPicker.background"                         : "#181E2B",

            // Button
            "button.background"                                   : "#FDD835",
            "button.foreground"                                   : "#181E2B",
            "button.hoverBackground"                              : "#FFF176",

            // Checkbox
            "checkbox.background"                                 : "#ff0000",
            "checkbox.border"                                     : "#ff0000",
            "checkbox.foreground"                                 : "#ff0000",

            // Contrast
            //"contrastActiveBorder"                                : "#ff0000",
            //"contrastBorder"                                      : "#ff0000",

            // Debug Exception
            "debugExceptionWidget.background"                     : "#ff0000",
            "debugExceptionWidget.border"                         : "#ff0000",

            // Description
            "descriptionForeground"                               : "#CFD8DC",

            // Diff Editor
            "diffEditor.border"                                   : "#ff0000",

            // Diff Editor Inserted
            "diffEditor.insertedTextBackground"                   : "#ff0000",
            "diffEditor.insertedTextBorder"                       : "#ff0000",

            // Diff Editor Removed
            "diffEditor.removedTextBackground"                    : "#ff0000",
            "diffEditor.removedTextBorder"                        : "#ff0000",

            // Dropdown
            "dropdown.background"                                 : "#181E2B",
            "dropdown.border"                                     : "#181E2B",
            "dropdown.foreground"                                 : "#FDD835",
            "dropdown.listBackground"                             : "#181E2B",

            // ==> Editor.Def --------------------------------------------------

            // => Main Colors
            "editor.background"                                   : "#222835",
            "editor.foreground"                                   : "#CFD8DC",

            // => Other Colors
