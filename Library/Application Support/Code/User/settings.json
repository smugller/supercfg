            // JSX - JavaScript JSX
            //========================================================================================
            {
                "scope":
                [
                    "entity.name.tag.open.jsx",
                    "entity.name.tag.close.jsx",
                ],
                "settings":
                {
                    "foreground": "#FFA759",
                    "fontStyle": "bold"
                }
            },
            {
                "scope":
                [
                    "entity.other.attribute-name.jsx",
                ],
                "settings":
                {
                    "foreground": "#FFFFFF",
                    "fontStyle": "bold"
                }
            },
            {
                "scope":
                [
                    "entity.other.attribute-name.jsx",
